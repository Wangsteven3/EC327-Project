<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sleeptest">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SleepTest">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!-- define profile and alarm activity -->
        <activity android:name=".profile"></activity>
        <activity android:name=".alarm"></activity>
        <activity android:name=".Simon"></activity>
        <activity android:name=".gamemenu"></activity>
        <activity android:name=".memory2"></activity>
        <activity android:name=".memory3"></activity>
        <activity android:name=".memory2b"></activity>
        <activity android:name=".memory3b"></activity>
        <activity android:name=".memory4b"></activity>
        <activity android:name=".Simonb"></activity>
        <activity android:name=".memory4"></activity>
        <activity android:name=".scorepage"></activity>

        <!-- define alert receiver -->
        <receiver android:name=".AlertReceiver"/>

    </application>

</manifest>